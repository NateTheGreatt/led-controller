{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "server/**/*.js",
    "client/**/*.js"
  ],
  "plugins": {
    "node-express": {},
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "complete_strings": {
      "maxLength": 15
    },
    "doc_comment": {
      "fullDocs": true,
      "strong": false
    }
  }
}